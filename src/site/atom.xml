---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>DartLang.org News and Updates</title>
  <link href="http://www.dartlang.org/"/>
  <link type="application/atom+xml" rel="self" href="http://www.dartlang.org/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.dartlang.org/</id>
  <author>
    <name>The Google Dart team</name>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://www.dartlang.org{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://www.dartlang.org{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>